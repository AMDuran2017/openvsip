srcdir := @srcdir@
top_srcdir:=@top_srcdir@

all:
	@echo making all in src
	@$(MAKE) -C src

clean:
	@echo making clean in src
	@$(MAKE) -C src clean

install:
	@$(MAKE) -C src install

depend:
	@$(MAKE) -C src depend
