srcdir := @srcdir@
top_srcdir:=@top_srcdir@

include ../config.mk

define install_dir
@echo installing $(1)
$(INSTALL) -d $(DESTDIR)$(pkgdatadir)/$(1)
for f in $(wildcard $(srcdir)/$(1)/*); do \
  $(INSTALL_DATA) $$f $(DESTDIR)$(pkgdatadir)/$(1); \
done
endef


all:

install:

clean:
